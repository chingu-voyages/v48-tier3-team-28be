{
  "name": "v48-tier3-team-28be",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "repository": "https://github.com/chingu-voyages/v48-tier3-team-28be.git",
  "author": "Chingu Team 23",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.local -- tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test:ci": "vitest",
    "test:local": "dotenv -e .env.local -- vitest",
    "test:gh:test": "act -j 'build' --secret-file .env.local",
    "db:seed": "tsx ./src/db/seed.mts",
    "db:reset": "supabase db reset",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "generate:stations": "tsx scripts/generateStationList.mts",
    "generate:readings": "tsx scripts/generateReadingList.mts",
    "db:client:generate": "prisma generate",
    "db:migrate:generate:local": "prisma migrate dev --create-only",
    "db:migrate:apply:local": "prisma migrate dev"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@types/node": "^20.11.28",
    "drizzle-kit": "^0.20.14",
    "prisma": "^5.11.0",
    "supabase": "^1.150.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@clerk/backend": "^0.38.3",
    "@hono/clerk-auth": "^1.0.1",
    "@hono/node-server": "^1.8.2",
    "@hono/zod-openapi": "^0.9.8",
    "@prisma/client": "5.11.0",
    "dotenv-cli": "^7.4.1",
    "drizzle-orm": "^0.30.4",
    "hono": "^4.1.0",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  }
}
